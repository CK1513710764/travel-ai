# Backend Environment Variables Template
# Copy this file to .env and fill in your actual values

# Server Configuration
NODE_ENV=development
PORT=5000

# Supabase Configuration
# 获取方式：Supabase Dashboard → Project Settings → API
# SUPABASE_URL: 项目的 API URL
# SUPABASE_ANON_KEY: anon/public key（可在客户端使用，受 RLS 保护）
# SUPABASE_SERVICE_ROLE_KEY: service_role key（⚠️ 仅服务端使用，绕过 RLS）
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Aliyun 百炼 API
ALIYUN_API_KEY=your-aliyun-api-key-here

# Amap (高德地图) API
AMAP_API_KEY=your-amap-api-key-here

# CORS (Frontend URL)
FRONTEND_URL=http://localhost:5173
